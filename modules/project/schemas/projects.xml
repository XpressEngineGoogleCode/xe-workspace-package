<table name="projects">
    <column name="site_srl" type="number" size="11" notnull="notnull" primary_key="primary_key" />
    <column name="title" type="varchar" size="255" notnull="notnull" />
    <column name="list_order" type="number" size="11" notnull="notnull" index="idx_list_order" />
    <column name="description" type="text"  />
    <column name="point" type="number"  size="11" index="idx_point" default="0" />
    <column name="rank" type="number"  size="11" index="idx_rank" default="0" />
    <column name="regdate" type="date" />
</table>

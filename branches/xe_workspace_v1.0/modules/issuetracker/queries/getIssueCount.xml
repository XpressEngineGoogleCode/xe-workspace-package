<query id="getIssueCount" action="select">
    <tables>
        <table name="issues" />
    </tables>
    <columns>
        <column name="count(*)" alias="count" />
    </columns>
    <conditions>
        <condition operation="equal" column="module_srl" var="module_srl" notnull="notnull" />
    </conditions>
</query>

<query id="insertIssueDownloadHistory" action="insert">
    <tables>
        <table name="issue_download_history" />
    </tables>
    <columns>
        <column name="release_srl" var="release_srl" filter="number" notnull="notnull" />
        <column name="file_srl" var="file_srl" filter="number" notnull="notnull" />
        <column name="member_srl" var="member_srl" default="0" filter="number" notnull="notnull" />
        <column name="download_date" default="curdate()" filter="number" notnull="notnull" />
    </columns>
</query>
